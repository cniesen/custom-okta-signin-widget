{
  "name": "custom-okta-signin-widget",
  "version": "1.0.0",
  "description": "Customized Okta Sign-in Widget",
  "main": "src/script.js",
  "repository": "git@git.its.iastate.edu:scm/okta/iastate-login.git",
  "scripts": {
    "build-dev": "ncp config-dev.json config.json && cross-env NODE_ENV=production webpack && ncp src/static/ dist/ --filter=.*",
    "build-prod": "ncp config-prod.json config.json && cross-env NODE_ENV=production webpack && ncp src/static/ dist/ --filter=.*",
    "start": "ncp config-dev.json config.json && node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js"
  },
  "author": "Claus Niesen",
  "license": "UNLICENSED",
  "private": true,
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "babel-loader": "^8.0.6",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "file-loader": "^3.0.1",
    "ncp": "^2.0.0",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.31.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-middleware": "^3.6.2",
    "webpack-dev-server": "^3.3.1",
    "webpack-hot-middleware": "^2.24.4"
  },
  "dependencies": {
    "query-string": "^6.5.0"
  }
